{
  "private": true,
  "name": "purescript-grain-router",
  "scripts": {
    "spago": "spago",
    "watch": "spago build -w -u '--strict --stash --censor-lib --is-lib=.spago'",
    "clean": "rm -rf output",
    "test": "spago -x test.dhall test -u '--strict --stash --censor-lib --is-lib=.spago'",
    "start": "spago -x example.dhall build -u '--strict --stash --censor-lib --is-lib=.spago' && parcel ./example/index.html",
    "pulp:publish": "pulp publish",
    "pulp:login": "pulp login"
  },
  "dependencies": {
    "bower": "^1.8.8",
    "parcel": "^1.12.4",
    "pulp": "^15.0.0",
    "purescript": "^0.13.8",
    "purescript-psa": "^0.7.3",
    "spago": "^0.15.3"
  }
}
